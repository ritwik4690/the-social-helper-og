<%- include('partials/header') %>
<section id="feed" style="background-color: wheat;">
<div id=mario-chat>
    <div id="chat-window">
        <div id="output"></div>
        <div id="feedback"></div>
    </div>
    <input type="text" id="handle" placeholder="Handle" value="<%= name %>" >
    <input type="text" id="message" placeholder="Message"> 
    <input type="text" id="sendto" placeholder="To:">
    <button id="send">Send</button>
</div>

        <a class="btn btn-light" role="button" href="/logout">Logout</a>
</section>
    
